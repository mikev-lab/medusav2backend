#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mike/Documents/medusav2backend/node_modules/.pnpm/@medusajs+cli@2.12.2_@types+node@25.0.1/node_modules/@medusajs/cli/node_modules:/Users/mike/Documents/medusav2backend/node_modules/.pnpm/@medusajs+cli@2.12.2_@types+node@25.0.1/node_modules/@medusajs/node_modules:/Users/mike/Documents/medusav2backend/node_modules/.pnpm/@medusajs+cli@2.12.2_@types+node@25.0.1/node_modules:/Users/mike/Documents/medusav2backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mike/Documents/medusav2backend/node_modules/.pnpm/@medusajs+cli@2.12.2_@types+node@25.0.1/node_modules/@medusajs/cli/node_modules:/Users/mike/Documents/medusav2backend/node_modules/.pnpm/@medusajs+cli@2.12.2_@types+node@25.0.1/node_modules/@medusajs/node_modules:/Users/mike/Documents/medusav2backend/node_modules/.pnpm/@medusajs+cli@2.12.2_@types+node@25.0.1/node_modules:/Users/mike/Documents/medusav2backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
